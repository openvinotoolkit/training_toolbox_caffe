name: "RMNet_LReLU with SSD head for Action Detection and Action Recognition"

layer {
  type: "Python"
  name: "data"
  top: "data"
  top: "label"
  include {
    phase: TRAIN
  }
  python_param {
    module: "caffe.custom_layers.actions_data_layer"
    layer: "ActionsDataLayer"
    param_str:  '{"batch": 4, "tasks": "train_tasks.txt", "valid_action_ids": [0, 1, 2, 3, 4, 5], "ignore_class_id": 5, "height": 400, "width": 680, "ignore_occluded": False, "num_data_fillers": 2, "data_queue_size": 20, "gamma": True, "delta": 0.1, "mirror": True, "down_up_scale": True, "noise": False, "salt_pepper": False, "free_weight": 0.1, "expand_weight": 1.0, "crop_weight": 1.0}'
  }
}

###################################################
############### Data normalization ################
###################################################

layer {
  name: "data/norm/bn"
  type: "BatchNorm"
  bottom: "data"
  top: "data/norm/bn"
  param {
    lr_mult: 0.0
    decay_mult: 0.0
  }
  param {
    lr_mult: 0.0
    decay_mult: 0.0
  }
  param {
    lr_mult: 0.0
    decay_mult: 0.0
  }
  batch_norm_param {
    use_global_stats: true
  }
}
layer {
  name: "data/norm/scale"
  type: "Scale"
  bottom: "data/norm/bn"
  top: "data/norm/bn"
  param {
    lr_mult: 0.0
    decay_mult: 0.0
  }
  param {
    lr_mult: 0.0
    decay_mult: 0.0
  }
  scale_param {
    axis: 1
    filler {
      value: 1.0
    }
    bias_term: true
    bias_filler {
      value: 0.0
    }
  }
}

