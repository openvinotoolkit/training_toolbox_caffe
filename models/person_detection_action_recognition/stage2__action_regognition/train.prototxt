  propagate_down: true
  propagate_down: false
  loss_param: {
    ignore_label: -1
    normalization: VALID
    adaptive_weighting: true
    scale: 1.0
    gamma: 0.9
    min_weight: 0.0001
    max_weight: 100.0
    max_entropy_weight: 0.4
