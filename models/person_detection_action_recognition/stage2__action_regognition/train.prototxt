  python_param {
    module: "caffe.custom_layers.schedule_scale_layer"
    layer: "ScheduledScaleLayer"
    param_str:   '{"init_scale": 16, "target_scale": 3.0, "num_steps": 100000, "power": 1.2, "use_last": False}'
  }
}

###################################################
########## Margin-based Global Losses #############
###################################################

layer {
  name: "action/sm_loss"
  type: "SoftmaxWithLoss"
  bottom: "action/logits"
  bottom: "action/labels"
