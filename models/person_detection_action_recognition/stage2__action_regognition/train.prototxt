#################################################

layer {
  type: "AnchorEmbeddingExtractor"
  name: "action/embeddings/extractor"
  bottom: "matched_detections"
  bottom: "action/anchor1/norm"
  bottom: "action/anchor2/norm"
  bottom: "action/anchor3/norm"
  bottom: "action/anchor4/norm"
  top: "action/embeddings"
  top: "action/labels"
  propagate_down: false
  propagate_down: true
  propagate_down: true
  propagate_down: true
  propagate_down: true
  embedding_extractor_param {
    num_anchors: 4
    label_src_pos: 8
    num_valid_actions: 5
  }
}
layer {
  name: "centers/action/params"
  type: "Parameter"
  top: "centers/action"
  param {
    lr_mult: 1.0
    decay_mult: 0.0
  }
  parameter_param {
    shape: {
      dim: 5  # num_actions == 5
      dim: 128
    }
    filler: {
      type: "gaussian"
      mean: 0.0
      std: 1.0
    }
  }
}
layer {
  name: "centers/action/norm"
  type: "GRN"
  bottom: "centers/action"
  top: "centers/action/norm"
  grn_param {
    bias: 0.00000001
  }
}

layer {
  name: "action/logits/fc"
  type: "InnerProduct"
  bottom: "action/embeddings"
