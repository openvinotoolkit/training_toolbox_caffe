{
  "batch": 4,
  "tasks": "<path_to_datasets>/train_tasks.txt",
  "height": 400,
  "width": 680,
  "ignore_occluded": false,
  "num_data_fillers": 2,
  "data_queue_size": 20,
  "gamma": true,
  "delta": 0.1,
  "mirror": true,
  "down_up_scale": false,
  "noise": false,
  "salt_pepper": false,
  "free_weight": 0.1,
  "expand_weight": 1.0,
  "crop_weight": 1.0,
  "use_attribute": "action",
  "crop_ratio_limits": [0.8, 1.2],
  "max_expand_ratio": 1.3,
  "valid_class_ids": [0, 1, 2, 3],
  "ignore_class_id": 3,
  "class_names_map": {
    "standing": 0,
    "speaking": 0,
    "writing": 1,
    "demonstrating": 2,
    "interacting": 0,
    "__undefined__": 3,
    },
  "valid_class_names": ["standing", "writing", "demonstrating"]
}
